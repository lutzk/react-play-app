{
  "main": "bin/server.js",
  "scripts": {
    "lf": "tslint -p tsconfig.json --fix",
    "check-app-server": "node -r esm ./check.server app",
    "check-api-server": "node -r esm ./check.server api",
    "dev-app-server-watch": "webpack -r esm --env dev.appServer --colors --display-error-details --config webpack/webpackConfig.js --watch",
    "dev-api-server-watch": "webpack -r esm --env dev.apiServer --colors --display-error-details --config webpack/webpackConfig.js --watch",
    "dev-cache-worker": "webpack -r esm --env dev.worker.cache --colors --display-used-exports --display-error-details --config webpack/webpackConfig.js --watch",
    "dev-pouch-worker": "webpack -r esm --env dev.worker.pouch --colors --display-used-exports --display-error-details --config webpack/webpackConfig.js --watch",
    "dev-client-watch-server": "node -r esm webpack/webpackDevServer.js",
    "prod-app-server": "webpack --env prod.appServer --display-used-exports --display-error-details --config webpack/webpackConfig.js",
    "prod-api-server": "webpack --env prod.apiServer --display-used-exports --display-error-details --config webpack/webpackConfig.js",
    "prod-client": "webpack --env prod.client --colors --display-used-exports --display-error-details --config webpack/webpackConfig.js",
    "prod-worker": "webpack --env prod.worker --colors --display-used-exports --display-error-details --config webpack/webpackConfig.js",
    "run-dev-app-server-watch": "npm-run-all --parallel check-app-server dev-app-server-watch",
    "run-dev-api-server-watch": "npm-run-all --parallel check-api-server dev-api-server-watch",
    "build-prod-all": "npm-run-all --parallel prod-app-server prod-api-server prod-client",
    "start-prod-app-server": "node ./startProdAppServer",
    "start-prod-api-server": "node ./startProdApiServer",
    "start-prod": "npm-run-all --parallel start-prod-app-server start-prod-api-server",
    "dev-all": "export NODE_ENV=development; npm-run-all --parallel run-dev-app-server-watch run-dev-api-server-watch dev-client-watch-server dev-pouch-worker"
  },
  "devDependencies": {
